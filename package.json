{
  "name": "man-up",
  "private": true,
  "description": "1-pager for Man Up group, sub-branch of Refreshing Word Church",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:prod",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "build:prod": "npm run build:css && remix build",
    "dev": "concurrently \"npm run dev:css\" \"npm run remix:dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev:dotenv": "cross-env NODE_ENV=development node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "remix:dev": "cross-env NODE_ENV=development remix dev"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-wesbos": "^3.0.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  }
}
